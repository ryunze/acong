{
    "name": "Acong",
    "description": "CBOSS Mod",
    "version": "1.0",
    "manifest_version": 3,
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "action": {
        "default_popup": "index.html"
    },
    "permissions": [
        "storage",
        "scripting",
        "tabs"
    ],
    "host_permissions": ["https://cboss.kreditpintar.com/*"],
    "content_scripts": [{
        "matches": ["https://cboss.kreditpintar.com/*"],
        "css": ["themes/default/theme.css", "content.css"],
        "js": ["core.js", "content.js"],
        "run_at": "document_idle"
    }]
}